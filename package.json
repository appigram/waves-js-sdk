{
  "name": "@wavesenterprise/js-sdk",
  "version": "3.1.7",
  "description": "Waves Enterprise client-side API library",
  "keywords": [
    "cryptocurrency",
    "waves",
    "api",
    "seed",
    "transactions",
    "cryptography",
    "signature"
  ],
  "author": {
    "name": "Kuvakin Aleksandr",
    "email": "sa-ny_95@mail.ru",
    "url": "https://github.com/larsan12"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wavesplatform/js-sdk.git"
  },
  "main": "raw/src/index.js",
  "types": "raw/src/index.d.ts",
  "devDependencies": {
    "@types/chai": "4.1.5",
    "@types/google-protobuf": "^3.7.4",
    "@types/mocha": "5.2.5",
    "browserify": "^17.0.0",
    "chai": "4.1.2",
    "mocha": "5.2.0",
    "mock-require": "3.0.2",
    "protoc-gen-grpc-web": "^1.2.1",
    "requirejs": "2.3.6",
    "ts-node": "^7.0.1",
    "tslint": "^6.1.3",
    "typescript": "4.0.2",
    "uglify-es": "3.3.9"
  },
  "scripts": {
    "build-full": "node_modules/.bin/browserify raw/src/index.js -u node-fetch -s WeSdk -o ./dist/we-sdk.js --insert-global-vars='__filename,__dirname' --full-paths",
    "build": "rm -rf ./raw && rm -rf ./dist && npm run compile && mkdir -p dist && npm run build-full && npm run uglifyjs && cp ./interfaces.d.ts ./raw/interfaces.d.ts",
    "compile": "mkdir -p raw && ./node_modules/.bin/tsc -p ./tsconfig.json --declaration --outDir raw && cp -R ./src/grpc/compiled-node ./raw/src/grpc/compiled-node && cp -R ./src/grpc/compiled-web ./raw/src/grpc/compiled-web",
    "uglifyjs": "node_modules/.bin/uglifyjs ./dist/we-sdk.js -o ./dist/we-sdk.min.js",
    "compile-grpc": "sudo chmod +x ./compile-proto.sh && ./compile-proto.sh",
    "test": "node examples/grpc-node/call-contract.js",
    "lint": "tslint --fix src/**/*.ts -t verbose"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.2.6",
    "@types/long": "^4.0.1",
    "@wavesenterprise/transactions-factory": "1.2.3-RC1",
    "bignumber.js": "7.2.1",
    "crypto-js": "3.1.9-1",
    "google-protobuf": "^3.14.0",
    "grpc-tools": "1.9.1",
    "grpc-web": "^1.2.1",
    "js-sha256": "^0.9.0",
    "long": "^4.0.0",
    "node-fetch": "2.2.0",
    "parse-json-bignumber": "^0.1.2",
    "protobufjs": "^6.10.2",
    "ts-api-validator": "2.1.3",
    "ts-protoc-gen": "^0.14.0",
    "ts-utils": "6.0.4"
  }
}
